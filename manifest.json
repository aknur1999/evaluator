{
  "name": "Traversy Launcher",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": ["evaluator.js"],
      "matches": ["https://www.alexa.com/*"],
      "run_at": "document_end"
    }
  ]
}
