{
  "manifest_version": 2,
  "name": "Traversy Launcher",
  "description": "Quick launch Traversy Media",
  "version": "1.0.0",
  "icons": {"128": "icon_128.png"},
  "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "tabs", "storage"],
  "content_scripts": [
    {
      "js": ["evaluator.js"],
      "matches": [
        "https://www.alexa.com/*"      ],
      "run_at": "document_end"
    }
]
}